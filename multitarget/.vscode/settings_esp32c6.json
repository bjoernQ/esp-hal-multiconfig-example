// copy this as `.vscode.settings` for the target/config you want to edit
{
    "rust-analyzer.cargo.target": "riscv32imac-unknown-none-elf",
    "rust-analyzer.cargo.features": [
        "esp32c6"
    ],
    "rust-analyzer.cargo.extraArgs": ["--config=./.cargo/config_esp32c6.toml"],
    "rust-analyzer.cargo.buildScripts.useRustcWrapper": true,
    "rust-analyzer.cargo.allTargets": false,
    "rust-analyzer.check.allTargets": false,
    "rust-analyzer.showUnlinkedFileNotification": false,
}
